{
    "name": "phariscope/event-rabbitmq-publisher",
    "type": "library",
    "description": "Publish stored events via RabbitMq",
    "keywords": ["event","RabbitMQ","DDD"],
    "homepage": "https://github.com/phariscope/EventRabbitMqPublisher",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Frédéric Royet",
            "email": "frederic.royet@logipro.com",
            "homepage": "https://www.linkedin.com/in/frederic-royet-42568566/",
            "role": "Project Founder"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phariscope/event-store-doctrine": "^1.0"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.27",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "1.10.15",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4" : {
            "Phariscope\\EventRabbitMqPublisher\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Phariscope\\EventRabbitMqPublisher\\Tests\\" : "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
